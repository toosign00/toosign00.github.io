{
  "name": "hyunsoo-portfolio",
  "private": true,
  "version": "2.1.3",
  "type": "module",
  "module": "ES2022",
  "description": "프론트엔드 개발자 노현수의 포트폴리오 웹사이트",
  "author": "Hyunsoo Roh",
  "license": "MIT",
  "homepage": "https://toosign.kr/",
  "repository": {
    "type": "git",
    "url": "https://github.com/toosign00/portfolio"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "frontend",
    "developer"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "compile": "pnpm clean && pnpm build && pnpm preview",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "pnpm type-check && biome check --write",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "reporter": "biome check --reporter=summary"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@supabase/supabase-js": "^2.50.2",
    "@tanstack/react-query": "^5.81.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "es-toolkit": "^1.39.5",
    "framer-motion": "^12.19.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "globals": "^16.2.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^7.0.0"
  }
}
