{
  "name": "hyunsoo-portfolio",
  "private": true,
  "version": "2.1.3",
  "type": "module",
  "description": "프론트엔드 개발자 노현수의 포트폴리오 웹사이트",
  "author": "Hyunsoo Roh",
  "license": "MIT",
  "homepage": "https://toosign.kr/",
  "repository": {
    "type": "git",
    "url": "https://github.com/toosign00/portfolio"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "frontend",
    "developer"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "compile": "pnpm clean && pnpm build && pnpm preview",
    "check": "concurrently \"pnpm:type-check\" \"pnpm:lint\" \"pnpm:format\"",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "format": "prettier --check . --cache",
    "format:fix": "prettier --write . --cache",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext ts,tsx --fix --cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@supabase/supabase-js": "^2.50.1",
    "@tanstack/react-query": "^5.81.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "es-toolkit": "^1.39.5",
    "framer-motion": "^12.19.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5"
  }
}
